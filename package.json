{
  "name": "aws-local-dev",
  "version": "0.1.0",
  "bin": {
    "aws-local-dev": "bin/aws-local-dev.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test-units": "jest",
