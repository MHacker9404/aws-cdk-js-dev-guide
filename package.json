  "devDependencies": {
    "@aws-cdk/assert": "1.42.0",
    "@types/jest": "^25.2.1",
    "@types/node": "10.17.5",
    "aws-cdk": "1.42.0",
    "aws-sdk": "^2.686.0",
    "jest": "^25.5.0",
    "js-yaml": "^3.14.0",
    "prompt": "^1.0.0",
    "ts-jest": "^25.3.1",
    "ts-node": "^8.1.0",
    "typescript": "~3.7.2"
  },
